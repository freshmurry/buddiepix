    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale = 1.0, maximum-scale=1.0, user-scalable=no" />
    
    <title>FreshMurry • Capture, edit & share photos, videos & messages with friends & family</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= display_meta_tags site: 'FreshMurry • Capture & share photos with friends & family.',
                          keywords: %w[Photo Sharing, Fake Instgram, Instafake, Finsta],
                          description: "Create an account or log in to FreshMurry - A simple, fun & creative way to capture & share photos, videos & messages with friends & family."%>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
   	<meta name="author" content="FreshMurry" />
    <meta name="keywords" content="photo sharing app, finsta" />
    <meta name="description" content="FreshMurry • Capture & share photos with friends & family.">
    <meta name="google-site-verification" content="" />
    <meta property="og:url" content="http://freshmurry.com" />
    <meta property="og:title" content="FreshMurry | Photo Sharing Website."/>
    <meta property="og:description" content="FreshMurry • Capture & share photos with friends & family.." />
    <meta property="og:type" content="website"/>
    <meta property="og:locale" content="en_US" />
    <meta property="og:image" content="https://res.cloudinary.com/peerspace-inc/image/upload/k1e3susxysl1zr7aqpvi.jpg" />
    
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:url" content="http://freshmurry.com">
    <meta name="twitter:title" content="FreshMurry" />
    <meta property="twitter:description" content="FreshMurry • Capture & share photos with friends & family." />
    <meta name="twitter:image" content="https://res.cloudinary.com/peerspace-inc/image/upload/k1e3susxysl1zr7aqpvi.jpg" />
    
    <meta name="facebook:card" content="summary" />
    <meta name="facebook:url" content="http://freshmurry.com">
    <meta property="fb:app_id" content="" />
    <meta name="facebook:title" content="FreshMurry • Capture & Share Photos" />
    <meta property="facebook:description" content="FreshMurry • Capture & share photos with friends & family." />
    <meta name="facebook:image" content="https://res.cloudinary.com/peerspace-inc/image/upload/k1e3susxysl1zr7aqpvi.jpg" />
    <meta property="og:image:alt" content="Photo Sharing Website" />
    <meta name="" content="" />
        
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"/>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/smoothness/jquery-ui.css">
    <%= favicon_link_tag 'icon.jpg' %>

    <!-- Global site tag (gtag.js) - Google Analytics -->
